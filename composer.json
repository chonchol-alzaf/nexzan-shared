{
    "name": "nexzan/shared",
    "description": "Shared package for all Nexzan services",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nexzan\\Shared\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alzaf-package/microservice-auth"
        }
    ],
    "require": {
        "php": ">=8.1",
        "bensampo/laravel-enum": "^6.7",
        "alzaf-package/microservice-auth": "v1.0.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nexzan\\Shared\\Providers\\NexzanSharedServiceProvider"
            ]
        }
    }
}